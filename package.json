{
	"name": "eizen",
	"version": "1.0.0",
	"description": "Vector database backend Engine lib for ArchiveNet",
	"main": "dist/index.js",
	"type": "module",
	"scripts": {
		"build": "tsdown",
		"test": "vitest",
		"test:ui": "vite --ui",
		"coverage": "vitest run --coverage",
		"dev": "tsx src/index.ts",
		"lint": "biome lint .",
		"format": "biome format --write .",
		"check": "biome check --write .",
		"check:ci": "biome check .",
		"proto:clean": "rm -rf proto/*.js proto/*.d.ts",
		"proto:gen": "npm run proto:clean && npx pbjs ./proto/hnsw_comm.proto -w commonjs -t static-module -o ./proto/hnsw_comm.js && npx pbts ./proto/hnsw_comm.js -o ./proto/hnsw_comm.d.ts && npx pbjs ./proto/request.proto -w commonjs -t static-module -o ./proto/request.js && npx pbts ./proto/request.js -o ./proto/request.d.ts"
	},
	"keywords": [],
	"author": "Agasta",
	"license": "ISC",
	"packageManager": "pnpm@10.11.0",
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@tsconfig/node23": "^23.0.2",
		"@types/long": "^5.0.0",
		"@types/node": "^22.15.21",
		"@vitest/coverage-v8": "^3.1.4",
		"@vitest/ui": "^3.1.4",
		"protobufjs-cli": "^1.1.3",
		"tsdown": "^0.12.4",
		"tsx": "^4.19.4",
		"typescript": "^5.8.3",
		"vitest": "^3.1.4"
	},
	"dependencies": {
		"heap-js": "^2.6.0",
		"hollowdb": "^1.4.3",
		"long": "^5.3.2",
		"protobufjs": "^7.4.0",
		"warp-contracts": "^1.4.45",
		"warp-contracts-plugin-deploy": "^1.0.13"
	}
}
